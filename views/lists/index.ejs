<%- include('../partials/header') %>
<a class="add-list" href="/<%= user._id %>/lists/new">Add a List</a>
<div class="list">
    <table>
        <thead>
            <tr>
                <th>Title</th>
            </tr>
        </thead>
        <tbody>
            <% lists.forEach(function (list){ %>
                <% if (user?._id.equals(list.user)) {%>
                <tr>
                    <td>
                        <a href="/<%= user._id %>/lists/<%= list.id %>">
                            <%= list.title %>
                        </a>
                    </td>
                </tr>
                <% } %>
                <% }) %>
            </tbody>
    </table>
</div>
<%- include('../partials/footer') %>